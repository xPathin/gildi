{
    "name": "gildi-univ3-price-mirror",
    "version": "1.0.0",
    "description": "A TypeScript application that fetches price data from APIs and manages Uniswap V3 liquidity on-chain",
    "main": "dist/index.js",
    "scripts": {
        "typechain": "typechain --target ethers-v6 --out-dir src/typechain-types 'abi/**/*.json'",
        "prebuild": "npm run typechain",
        "predev": "npm run typechain",
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "clean": "rimraf dist src/typechain-types",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "docker:build": "docker build -t gildi-univ3-price-mirror .",
        "docker:run": "docker run -p 3000:3000 gildi-univ3-price-mirror",
        "docker:compose": "docker-compose up --build"
    },
    "keywords": [
        "typescript",
        "uniswap",
        "defi",
        "price-data",
        "coingecko",
        "liquidity",
        "on-chain"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.14.10",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "eslint": "^8.57.0",
        "ethers": "^6.15.0",
        "jsbi": "3.2.5",
        "rimraf": "^5.0.7",
        "typechain": "^8.3.2"
    },
    "dependencies": {
        "@typechain/ethers-v6": "^0.5.1",
        "dotenv": "^16.4.5",
        "axios": "^1.7.2",
        "tsx": "^4.16.2",
        "typescript": "^5.5.3",
        "@uniswap/sdk-core": "^7.7.2",
        "@uniswap/v3-sdk": "^3.25.2"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.15.6"
}